<service>
    <id>chromeHold</id>
    <name>chrome_hold</name>
    <description>浏览器自动重启服务</description>

    <!-- 可执行文件路径 -->
    <executable>chrome_hold.exe</executable>

    <!-- 启动参数（如果有的话） -->
    <arguments></arguments>

    <!-- 开机启动 -->
    <startmode>Automatic</startmode>

    <!-- 日志输出路径 -->
    <logpath>logs</logpath>
    <logmode>roll</logmode>
</service>